@model List<Customer>

@{
    ViewData["Title"] = "Home Page";
    int Roecount = 1;
}

<table>
    <tr>
        <th>#</th>
        <th>نام</th>
        <th></th>
        <th></th>
        <th></th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Roecount</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>

        </tr>
        Roecount++;
    }
</table>


